{
  "primary": {
    "$type": "fontFamilies",
    "$value": "{global.font.fontFamilies.ivyora}"
  },
  "secondary": {
    "$type": "fontFamilies",
    "$value": "{global.font.fontFamilies.gal-gothic}"
  },
  "lizzie": {
    "$type": "typography",
    "$value": {
      "fontFamily": "{primary}",
      "fontWeight": "{global.font.fontWeight.light}",
      "fontSize": "{global.dimensions.10}",
      "lineHeight": "{global.font.lineHeight.120}"
    }
  },
  "charlie": {
    "$type": "typography",
    "$value": {
      "fontFamily": "{secondary}",
      "fontWeight": "{global.font.fontWeight.regular}",
      "fontSize": "{global.font.fontSize.xs}",
      "lineHeight": "{global.font.lineHeight.140}"
    }
  }
}